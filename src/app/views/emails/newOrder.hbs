                                          <span><strong>Destinatário:</strong> {{user}}</span>
                                          <span><strong>Logradouro:</strong> {{street}}</span>
                                          <span><strong>Complemento:</strong> {{complement}}</span>
                                          <span><strong>Bairro:</strong> {{neighborhood}}</span>
                                        </div>
                                        <div style="display:flex;flex-direction:column;">
                                          <span><strong>CEP:</strong> {{postalCode}}</span>
                                          <span><strong>Número:</strong> {{streetN}}</span>
                                          <span><strong>Referência:</strong> {{reference}}</span>
                                          <span><strong>Cidade:</strong> {{city}}/{{state}}</span>
                                        </div>
                                      </div>
                                    </div>
                                </td>
                              </tr>
